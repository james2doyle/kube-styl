	width: math((((var(gridWidth) - (var(gridGutterWidth) * (5 - 1))) / 5 / (var(gridWidth) / 100)) * 4) + ((var(gridGutterWidth) / (var(gridWidth) / 100)) * (4 - 1)) * 1%);
